# Variáveis de Desenvolvimento
VITE_API_BASE_URL=http://localhost:3001/api
VITE_APP_NAME=Incuca Geek Jokes
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=SPA de humor baseado em estados emocionais

# Configurações da API
VITE_API_TIMEOUT=10000
VITE_API_RETRY_ATTEMPTS=3

# Configurações de Debug
VITE_DEBUG_MODE=true
VITE_ENABLE_LOGS=true

# Configurações de Cache
VITE_CACHE_ENABLED=true
VITE_CACHE_DURATION=300000

# Configurações de UI
VITE_THEME_DEFAULT=light
VITE_ANIMATION_DURATION=300
VITE_TOAST_DURATION=4000

# Features Flags
VITE_ENABLE_CONFETTI=true
VITE_ENABLE_PARTICLES=true
VITE_ENABLE_SOUND_EFFECTS=false
