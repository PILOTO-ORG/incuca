{
  "name": "incuca-frontend",
  "version": "1.0.0",
  "description": "Frontend SPA para o teste Incuca - SPA das Piadas Geek",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "vue": "^3.3.11",
    "vue-router": "^4.2.5",
    "pinia": "^2.1.7",
    "vuetify": "^3.4.8",
    "@mdi/font": "^7.3.67",
    "axios": "^1.6.2",
    "vue-toastification": "^2.0.0-rc.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.2",
    "vite": "^5.0.8",
    "vitest": "^1.0.4",
    "@vue/test-utils": "^2.4.2",
    "jsdom": "^23.0.1",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "eslint": "^8.55.0",
    "eslint-plugin-vue": "^9.19.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "prettier": "^3.1.1",
    "sass": "^1.69.5"
  },
  "keywords": [
    "vue",
    "vuetify",
    "pinia",
    "spa",
    "incuca"
  ],
  "author": "Incuca Test",
  "license": "MIT"
}
